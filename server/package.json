{
  "name": "final-project-senior-squad-1-server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha test/integration.js",
    "test-register": "npm run test -- -g 'register-test'",
    "test-login": "npm run test -- -g 'login-test'",
    "test-logout": "npm run test -- -g 'logout-test'",
    "test-exercise": "npm run test -- -g 'exercise-test'",
    "test-macro": "npm run test -- -g 'macro-test'",
    "test-nutrition": "npm run test -- -g 'nutrition-test'",
    "start": "cross-env DEBUG=finalproj-server* node ./bin/www",
    "test-for-coverage": "npm test -- -R min || exit 0",
    "style": "npm run test -- test/style.js",
    "coverage": "cross-env NODE_ENV=test nyc --check-coverage=true --reporter=lcov --reporter=text npm run test-for-coverage"
  },
  "dependencies": {
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "mongodb": "^3.5.5",
    "mongoose": "^5.9.2",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "axios": "^0.19.2",
    "axios-cookiejar-support": "^0.5.1",
    "cross-env": "^7.0.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "glob": "^7.1.6",
    "mocha": "^7.0.1",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1",
    "tough-cookie": "^3.0.1"
  },
  "nyc": {
    "branches": 80,
    "lines": 80,
    "functions": 100,
    "statements": 80
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  }
}
